<script>
  // ラジオボタンでフォームの表示・非表示切り替え
  var radio_form_box = document.getElementById('switch_radio_form');
  var check_radio_form = document.getElementsByClassName('switch_radio_form')
  var check_selected = document.getElementsByClassName('radio_selected')
  function radioBtnSwitch() {
    if (check_radio_form[0].checked) {
      radio_form_box.style.visibility = "";
    }else if(check_radio_form[1].checked) {
      radio_form_box.style.visibility = "hidden";
      for(var i=0; check_selected.length > i; i++){
        check_selected[i].checked = false;
      }
    }
  }
  // チェックボックスでフォームの表示・非表示切り替え
  var txt_form_box = document.getElementById('switch_txt_form');
  var check_txt_form = document.getElementsByClassName('switch_txt_form')
  var check_written = document.getElementsByClassName('written')
  function checkBoxSwitch() {
    if (check_txt_form[0].checked) {
      txt_form_box.style.visibility = "";
    }else if(!check_txt_form[0].checked) {
      txt_form_box.style.visibility = "hidden";
      check_written[0].value = '';
    }
  }
</script>
