<% case @document.document_type %>
<% when 'cover_document' %>
  <%= render 'users/documents/cover_documents/show' %> <!-- (1)表紙 -->
<% when 'table_of_contents_document' %>
  <%= render 'users/documents/table_of_contents_documents/show' %> <!-- (2)目次 -->
<% when 'doc_3rd' %>
  <%= render 'users/documents/doc_3rd/show' %> <!-- (3)施工体制台帳作成建設工事の通知 -->
<% when 'doc_4th' %>
  <%= render 'users/documents/doc_4th/show' %> <!-- (4)施工体制台帳 -->
<% when 'doc_5th' %>
  <%= render 'users/documents/doc_5th/show' %> <!-- (5)全建統一様式第１号－甲(再下請負通知書(変更届)) -->
<% when 'doc_6th' %>
  <%= render 'users/documents/doc_6th/show' %> <!-- (6)安全衛生管理に関する契約書(一次会社用) -->
<% when 'doc_7th' %>
  <%= render 'users/documents/doc_7th/show' %> <!-- (7)安全衛生管理に関する契約書(再下請会社用) -->
<% when 'doc_8th' %>
  <%= render 'users/documents/doc_8th/show' %> <!-- (8)作業員名簿 -->
<% when 'doc_9th' %>
  <%= render 'users/documents/doc_9th/show' %> <!-- (9)全建統一様式第１号-甲-別紙(外国人建設就労者建設現場入場届出書) -->
<% when 'doc_10th' %>
  <%= render 'users/documents/doc_10th/show' %> <!-- (10)高年齢者作業申告書 -->
<% when 'doc_11th' %>
  <%= render 'users/documents/doc_11th/show' %> <!-- (11)年少者就労報告書 -->
<% when 'doc_12th' %>
  <%= render 'users/documents/doc_12th/show' %> <!-- (12)工事用・通勤用車両届 -->
<% end %>
