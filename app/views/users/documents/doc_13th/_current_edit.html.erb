      <%= javascript_tag do %>
        <!-- 全選択・解除のチェックボックス -->
        let checkbox_all_crane<%= field_special_vehicle.id %> = document.querySelector(".checkbox_all_crane<%= field_special_vehicle.id %>");
        let checkbox_all_vehicle<%= field_special_vehicle.id %> = document.querySelector(".checkbox_all_vehicle<%= field_special_vehicle.id %>");
        <!-- チェックボックスのリスト -->
        let checkbox_list_crane<%= field_special_vehicle.id %> = document.querySelectorAll(".checkbox_list_crane<%= field_special_vehicle.id %>");
        let checkbox_list_vehicle<%= field_special_vehicle.id %> = document.querySelectorAll(".checkbox_list_vehicle<%= field_special_vehicle.id %>");
        <!-- 全選択のチェックボックスイベント -->
        checkbox_all_crane<%= field_special_vehicle.id %>.addEventListener('change', change_all_crane);
        checkbox_all_vehicle<%= field_special_vehicle.id %>.addEventListener('change', change_all_vehicle);
        
        function change_all_crane() {
          <!-- チェックされているか -->
          if (checkbox_all_crane<%= field_special_vehicle.id %>.checked) {
            <!-- 全て選択 -->
            for (let i in checkbox_list_crane<%= field_special_vehicle.id %>) {
              if (checkbox_list_crane<%= field_special_vehicle.id %>.hasOwnProperty(i)) {
                checkbox_list_crane<%= field_special_vehicle.id %>[i].checked = true;
              }
            }
          } else {
            <!-- 全て解除 -->
            for (let i in checkbox_list_crane<%= field_special_vehicle.id %>) {
              if (checkbox_list_crane<%= field_special_vehicle.id %>.hasOwnProperty(i)) {
                checkbox_list_crane<%= field_special_vehicle.id %>[i].checked = false;
              }
            }
          }
        };
        function change_all_vehicle() {
          <!-- チェックされているか -->
          if (checkbox_all_vehicle<%= field_special_vehicle.id %>.checked) {
            <!-- 全て選択 -->
            for (let i in checkbox_list_vehicle<%= field_special_vehicle.id %>) {
              if (checkbox_list_vehicle<%= field_special_vehicle.id %>.hasOwnProperty(i)) {
                checkbox_list_vehicle<%= field_special_vehicle.id %>[i].checked = true;
              }
            }
          } else {
            <!-- 全て解除 -->
            for (let i in checkbox_list_vehicle<%= field_special_vehicle.id %>) {
              if (checkbox_list_vehicle<%= field_special_vehicle.id %>.hasOwnProperty(i)) {
                checkbox_list_vehicle<%= field_special_vehicle.id %>[i].checked = false;
              }
            }
          }
        };
      <% end %>
