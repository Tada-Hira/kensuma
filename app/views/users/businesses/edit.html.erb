<% provide(:title, "事業所編集") %>
<% provide(:btn_text, "更新") %>

<div class="card">
  <div class="card-body">
    <%= form_with model: @business, url: users_businesses_path, method: :patch, local: true do |f| %>
      <%= render partial: "form", locals: { f: f } %>
  </div>
  <div class="card-footer">
      <%= f.submit yield(:btn_text), class: "btn btn-primary" %>
  </div>
    <% end %>
</div>